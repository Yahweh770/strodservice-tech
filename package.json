{
  "name": "strodservice",
  "version": "1.0.0",
  "description": "StrodService - Project containing backend, frontend and desktop application",
  "main": "electron-main.js",
  "scripts": {
    "start": "python main.py",
    "start:backend": "cd src/backend-python && uvicorn main:app --reload",
    "start:frontend": "cd src/frontend && npm start",
    "build:frontend": "cd src/frontend && npm run build",
    "build:desktop": "npm run build-frontend && npm run start-electron",
    "build-frontend": "cd src/frontend && npm run build",
    "prepackage-electron": "npm run build-frontend",
    "package-electron": "electron-builder --dir",
    "dist-electron": "electron-builder --config electron-builder-config.js",
    "postinstall": "cd src/frontend && npm install",
    "install-vendor": "bash install-vendor-deps.sh"
  },
  "keywords": [
    "construction",
    "management",
    "gpr",
    "pto"
  ],
  "author": "StrodService",
  "license": "MIT",
  "dependencies": {
    "react-router-dom": "^6.28.0",
    "@types/react-router-dom": "^5.3.3",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "concurrently": "^9.1.0"
  }
}
