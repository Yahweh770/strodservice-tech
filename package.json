{
  "name": "strodservice-project",
  "version": "1.0.0",
  "description": "Строд-Сервис Технолоджи - Project containing backend, frontend and desktop application",
  "main": "electron-main.js",
  "scripts": {
    "start": "python main.py",
    "start:backend": "cd src/backend-python && uvicorn main:app --reload",
    "start:frontend": "cd src/frontend && npm start",
    "build:frontend": "cd src/frontend && npm run build",
    "build:desktop": "npm run build-frontend && npm run start-electron",
    "build-frontend": "cd src/frontend && npm run build",
    "prepackage-electron": "npm run build-frontend",
    "package-electron": "electron-builder --dir",
    "dist-electron": "electron-builder --config electron-builder-config.js",
    "postinstall": "cd src/frontend && npm install"
  },
  "keywords": [
    "construction",
    "management",
    "gpr",
    "pto"
  ],
  "author": "Строд-Сервис Технолоджи",
  "license": "MIT",
  "dependencies": {
    "react-router-dom": "^6.8.1",
    "@types/react-router-dom": "^5.3.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "typescript": "^4.9.5",
    "concurrently": "^8.2.0"
  }
}
